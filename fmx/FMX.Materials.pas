procedure TLightMaterial.DoApply(const Context: TContext3D);
var
  Modulation: Single;
  MaxLightCount: Integer;
  I, CLight: integer;
  LLight: TLightDescription;
  PS: TContextShader;
begin
  // lights
  CLight := 0;
  for i := 0 to Context.Lights.Count - 1 do
    if Context.Lights[i].Enabled then
      Inc(CLight);
  CLight := Min(TContext3D.MaxLightCount, CLight);

  if Assigned(Texture) and (TTextureStyle.tsRenderTarget in Texture.Style) and (TContextStyle.RenderTargetFlipped in Context.Style) then
    PS := FPixelShaderFlip
  else
    PS := FPixelShader;

  case CLight of
    0: begin
      Context.SetShaders(FNoLightsVS, PS);
      MaxLightCount := 0;
    end;
    1: begin
      if TContext3D.MaxLightCount > 4 then
      begin
        Context.SetShaders(F2LightsVS, PS);
        MaxLightCount := 2;
      end else
      begin
        Context.SetShaders(FVertexShader, PS);
        MaxLightCount := 1;
      end;
    end;
    2: begin
      Context.SetShaders(F2LightsVS, PS);
      MaxLightCount := 2;
    end;
    3, 4: begin
      Context.SetShaders(F4LightsVS, PS);
      MaxLightCount := 4;
    end;
  else
    Context.SetShaders(F8LightsVS, PS);
    MaxLightCount := 8;
  end;
  // material
  Context.SetShaderVariable('MaterialDiffuse', FDiffuse);
  Context.SetShaderVariable('MaterialSpecular', FSpecular);
  Context.SetShaderVariable('MaterialAmbient', FAmbient);
  Context.SetShaderVariable('MaterialEmission', FEmissive);
  Context.SetShaderVariable('MaterialOpts', [Vector3D(FShininess, 0.0, 0, 0)]); // x - shininess
  Context.SetShaderVariable('EyePos', [Context.CurrentCameraMatrix.M[3]]);
  // lights
  if MaxLightCount > 0 then
  begin
    CLight := 0;
    for i := 0 to Min(MaxLightCount, Context.Lights.Count) - 1 do
      if Context.Lights[i].Enabled then
      begin
        LLight := Context.Lights[i];
        Context.SetShaderVariable('Lights[' + IntToStr(CLight) + '].Opts', [Vector3D(Integer(LLight.LightType) + 1, cos(DegToRad(LLight.SpotCutoff)), LLight.SpotExponent, 0)]);
        Context.SetShaderVariable('Lights[' + IntToStr(CLight) + '].Pos', [LLight.Position]);
        Context.SetShaderVariable('Lights[' + IntToStr(CLight) + '].Dir', [LLight.Direction]);
        Context.SetShaderVariable('Lights[' + IntToStr(CLight) + '].Color', [ColorToVector3D(LLight.Color)]);
        Inc(CLight);
      end;
    if CLight < MaxLightCount then
      for i := CLight to MaxLightCount - 1 do
        Context.SetShaderVariable('Lights[' + IntToStr(i) + '].Opts', [Vector3D(0, 0, 0, 0)]);
  end;
  // pixel shader
  // modulation, opacity
  if (FTexture = nil) or (FTexture.IsEmpty) then
    Modulation := 0
  else
    Modulation := 2;
  Context.SetShaderVariable('Opacity', [Vector3D(Context.CurrentOpacity, 0, 0, 0)]);
  Context.SetShaderVariable('Modulation', [Vector3D(Modulation, 0, 0, 0)]);
  Context.SetShaderVariable('texture0', FTexture);
end;

class function TLightMaterial.DoGetMaterialProperty(const Prop: TMaterial.TProperty): string;
begin
  case Prop of
    TProperty.ModelViewProjection: Result := 'MVPMatrix';
    TProperty.ModelView: Result := 'ModelView';
    TProperty.ModelViewInverseTranspose: Result := 'ModelViewIT';
  else
    Result := '';
  end;
end;

